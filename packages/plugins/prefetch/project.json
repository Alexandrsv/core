{
  "name": "prefetch",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/prefetch/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{workspaceRoot}/dist/packages/plugins/prefetch"],
      "options": {
        "parallel": false,
        "outputPath": "dist/packages/plugins/prefetch",
        "main": "packages/plugins/prefetch/src/index.ts",
        "additionalEntryPoints": [

        ],
        "tsConfig": "packages/plugins/prefetch/tsconfig.lib.json",
        "assets": [],
        "project": "packages/plugins/prefetch/package.json",
        "compiler": "swc",
        "rollupConfig": "packages/plugins/prefetch/rollup.config.js",
        "format": ["cjs", "esm"],
        "buildableProjectDepsInPackageJsonType": "peerDependencies",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/plugins/prefetch/**/*.ts",
          "packages/plugins/prefetch/package.json"
        ]
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run prefetch:test",
            "forwardAllArgs": false
          },
          {
            "command": "nx run prefetch:build",
            "forwardAllArgs": false
          },
          {
            "command": "nx run prefetch:semantic-release",
            "forwardAllArgs": true
          }
        ]
      }
    },
    "semantic-release": {
      "executor": "@goestav/nx-semantic-release:semantic-release"
    },
    "test": {
    }
  },
  "tags": ["package"]
}
