{
  "name": "runtime_demo_remote1",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/runtime-demo/remote1/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "swc",
        "outputPath": "dist/apps/runtime-demo/remote1",
        "index": "apps/runtime-demo/remote1/src/index.html",
        "baseHref": "/",
        "main": "apps/runtime-demo/remote1/src/index.ts",
        "tsConfig": "apps/runtime-demo/remote1/tsconfig.app.json",
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/runtime-demo/remote1/webpack.config.js",
        "babelUpwardRootMode": true
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "runtime_demo_remote1:build",
        "hmr": true,
        "port": 3006
      },
      "configurations": {
        "development": {
          "buildTarget": "runtime_demo_remote1:build:development"
        },
        "production": {
          "buildTarget": "runtime_demo_remote1:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/node-remote1/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/node-remote1/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
